<section class="header">
  <header class="centered">
    <img src="assets/title.png"/>
  </header>
</section>

<section class="middle-container">
  <section class="points-container centered">
    <img src="assets/points_200.png">
  </section>

  <section class="avatar-container centered">
    <img src=""/>
  </section>

  <div class="username centered">
    <p><%=name%></p>
  </div>
</section>

<footer>
  <div class="border-bottom"> 
    <img src="assets/text_border_bottom.png"/>
  </div>

  <div class="button centered">
    <img src="assets/next.png" class="shrink"/>
  </div>


  <div class="icons-container centered">
    <div class="icon-container user" data-id="menu" id="icon-menu"><img src="assets/menu-options.png" class="icon"/></div>
    <div class="icon-container table" data-id="scheduler" id="icon-scheduler"><img src="assets/menu-scheduler.png" class="icon"/></div>
    <div class="icon-container calendar" data-id="calendar" id="icon-calendar"><img src="assets/menu-calendar.png" class="icon"/></div>
    <div class="icon-container pin" data-id="pin" id="icon-pin"><img src="assets/menu-pin.png" class="icon"/></div>
    <div class="icon-container previous" data-id="exit" id="icon-exit"><img src="assets/menu-exit.png" class="icon"/></div>
  </div>
</footer>

  
